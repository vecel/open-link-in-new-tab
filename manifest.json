{

  "manifest_version": 2,
  "name": "Open link in a new tab",
  "version": "1.0",

  "description": "Opens every clicked link in a new tab.",

  "icons": {
    "48": "icons/open_new_tab_48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ]

}
